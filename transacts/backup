async function vericek_func(){

    console.log("\nVeri Çekiliyor..");
    var userDatas;
    let result;

    await getdata
            .listTable()
                .then((rows)=>{
                    userDatas=JSON.stringify(rows)
                    result=userDatas  
                })
                .catch((err) => { 
                    console.log( err); throw err })

    console.log("out finalyy")
    
    console.log(result)
     return{
        result:result
    }
}

